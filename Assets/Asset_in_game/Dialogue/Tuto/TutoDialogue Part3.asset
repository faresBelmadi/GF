    ReponsePossible:
    - IsEmotion: 0
      whichEmotion: 0
      SeuilClairvoyance: 0
      IDNextQuestion: 16
      "TexteR\xE9ponse": '!'
      "cons\xE9quences": []
  - ID: 16
    Question:
      IDSpeaker: 0
      type: 0
      Text: "Oh ! Tu as \xE9t\xE9 chanceux ! Il arrive parfois qu\u2019on tombe sur
